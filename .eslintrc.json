{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["react-app", "react-app/jest"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "off",
    "no-undef": "error",
    "semi": "off",
    "quotes": "off",
    "no-useless-escape": "warn",
    "no-unreachable": "warn",
    "no-dupe-keys": "error",
    "jsx-a11y/anchor-is-valid": "warn",
    "import/no-anonymous-default-export": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
